#include <unistd.h>
/**
* main - Entry point
* _putchar - writes the character c to stdout
* Return: Always 0 (Success)
* putchar - nested function
*/
int _putchar(void)
/* putchar - nested function 
* Return : Always zero
*/
{int _putchar;
int main(void)
{
putchar(_putchar());
putchar("\n");
return (0);
}
}
